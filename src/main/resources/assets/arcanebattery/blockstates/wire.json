{
    "multipart": [
        {   "when": { "north": true },
            "apply": { "model": "arcanebattery:cable_side" }
        },
        {   "when": { "east": true },
            "apply": { "model": "arcanebattery:cable_side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": true },
            "apply": { "model": "arcanebattery:cable_side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": true },
            "apply": { "model": "arcanebattery:cable_side", "y": 270, "uvlock": true }
        },
        {   "when": { "up": true },
            "apply": { "model": "arcanebattery:cable_side", "x": 270, "uvlock": true }
        },
        {   "when": { "down": true },
            "apply": { "model": "arcanebattery:cable_side", "x": 90, "uvlock": true }
        },
        {   "when": { "north": false },
            "apply": [
                { "model": "arcanebattery:cable_dot", "weight": 2 },
                { "model": "arcanebattery:cable_dot" },
                { "model": "arcanebattery:cable_dot" },
                { "model": "arcanebattery:cable_dot" }
            ]
        },
        {   "when": { "east": false },
            "apply": [
                { "model": "arcanebattery:cable_dot", "y": 90, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "y": 90, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "y": 90, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "weight": 2, "y": 90, "uvlock": true }
            ]
        },
        {   "when": { "south": false },
            "apply": [
                { "model": "arcanebattery:cable_dot", "y": 180, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "y": 180, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "weight": 2, "y": 180, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "y": 180, "uvlock": true }
            ]
        },
        {   "when": { "west": false },
            "apply": [
                { "model": "arcanebattery:cable_dot", "y": 270, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "weight": 2, "y": 270, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "y": 270, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "y": 270, "uvlock": true }
            ]
        },
        {   "when": { "up": false },
            "apply": [
                { "model": "arcanebattery:cable_dot", "weight": 2, "x": 270, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "x": 270, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "x": 270, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "x": 270, "uvlock": true }
            ]
        },
        {   "when": { "down": false },
            "apply": [
                { "model": "arcanebattery:cable_dot", "x": 90, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "x": 90, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "x": 90, "uvlock": true },
                { "model": "arcanebattery:cable_dot", "weight": 2, "x": 90, "uvlock": true }
            ]
        }
    ]
}
